  <span class="hljs-bullet">-</span> <span class="hljs-attr">action:</span> <span class="hljs-string">"updatePassword"</span>
    <span class="hljs-attr">conditions:</span>
      <span class="hljs-bullet">-</span> <span class="hljs-attr">name:</span> <span class="hljs-string">"validateOldPassword"</span>
        <span class="hljs-attr">type:</span> <span class="hljs-string">"function"</span>
        <span class="hljs-attr">value:</span> <span class="hljs-string">"validate_old_password"</span>
      <span class="hljs-bullet">-</span> <span class="hljs-attr">name:</span> <span class="hljs-string">"validateNewPassword"</span>
        <span class="hljs-attr">type:</span> <span class="hljs-string">"regex"</span>
        <span class="hljs-attr">value:</span> <span class="hljs-string">"^(?=.*[A-Z])(?=.*[a-z])(?=.*\\d)(?=.*[@$!%*?&amp;])[A-Za-z\\d@$!%*?&amp;]{8,}$"</span>
    <span class="hljs-attr">query:</span> <span class="hljs-string">"UPDATE users SET password = :newPassword WHERE username = :username"</span>

  <span class="hljs-bullet">-</span> <span class="hljs-attr">action:</span> <span class="hljs-string">"firstTimeLogin"</span>
    <span class="hljs-attr">conditions:</span>
      <span class="hljs-bullet">-</span> <span class="hljs-attr">name:</span> <span class="hljs-string">"isFirstTimeLogin"</span>
