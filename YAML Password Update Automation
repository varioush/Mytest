<span class="hljs-keyword">import</span> jakarta.inject.Inject;
<span class="hljs-keyword">import</span> jakarta.ws.rs.core.Response;

<span class="hljs-keyword">import</span> java.util.List;
<span class="hljs-keyword">import</span> java.util.Map;

<span class="hljs-meta">@ApplicationScoped</span>
<span class="hljs-keyword">public</span> <span class="hljs-keyword">class</span> <span class="hljs-title class_">RuleExecutionService</span> {

    <span class="hljs-meta">@Inject</span>
    DatabaseService dbService;

    <span class="hljs-keyword">public</span> Response <span class="hljs-title function_">executeRule</span><span class="hljs-params">(Map&lt;String, Object&gt; rule, Map&lt;String, Object&gt; inputs)</span> {
        List&lt;Map&lt;String, Object&gt;&gt; results;

        <span class="hljs-comment">// Execute Validations</span>
        <span class="hljs-keyword">if</span> (rule.containsKey(<span class="hljs-string">"validations"</span>)) {
            List&lt;Map&lt;String, String&gt;&gt; validations = (List&lt;Map&lt;String, String&gt;&gt;) rule.get(<span class="hljs-string">"validations"</span>);

            <span class="hljs-keyword">for</span> (Map&lt;String, String&gt; validation : validations) {
                <span class="hljs-type">String</span> <span class="hljs-variable">query</span> <span class="hljs-operator">=</span> validation.get(<span class="hljs-string">"query"</span>);
                <span class="hljs-type">String</span> <span class="hljs-variable">expectedResult</span> <span class="hljs-operator">=</span> validation.get(<span class="hljs-string">"expectedResult"</span>);

                results = dbService.executeQuery(query, inputs);

                <span class="hljs-keyword">if</span> (results.isEmpty() || !results.get(<span class="hljs-number">0</span>).values().contains(expectedResult)) {
                    <span class="hljs-keyword">if</span> (rule.containsKey(<span class="hljs-string">"error"</span>)) {
                        Map&lt;String, Object&gt; error = (Map&lt;String, Object&gt;) rule.get(<span class="hljs-string">"error"</span>);
                        <span class="hljs-type">int</span> <span class="hljs-variable">statusCode</span> <span class="hljs-operator">=</span> (<span class="hljs-type">int</span>) error.getOrDefault(<span class="hljs-string">"statusCode"</span>, <span class="hljs-number">400</span>);
                        <span class="hljs-type">String</span> <span class="hljs-variable">message</span> <span class="hljs-operator">=</span> (String) error.getOrDefault(<span class="hljs-string">"message"</span>, <span class="hljs-string">"Validation failed"</span>);
                        <span class="hljs-keyword">return</span> Response.status(statusCode).entity(message).build();
                    }
                    <span class="hljs-keyword">return</span> Response.status(Response.Status.BAD_REQUEST).entity(<span class="hljs-string">"Validation failed"</span>).build();
                }
            }
        }

        <span class="hljs-comment">// Execute Action</span>
        <span class="hljs-keyword">if</span> (rule.containsKey(<span class="hljs-string">"executionQuery"</span>)) {
            <span class="hljs-type">String</span> <span class="hljs-variable">query</span> <span class="hljs-operator">=</span> (String) rule.get(<span class="hljs-string">"executionQuery"</span>);
            results = dbService.executeQuery(query, inputs);
            <span class="hljs-keyword">return</span> Response.ok(results).build();
        }

        <span class="hljs-keyword">return</span> Response.status(Response.Status.NOT_IMPLEMENTED).entity(<span class="hljs-string">"Action not implemented"</span>).build();
    }
}
</code></div></div></pre><hr><hr><h1><strong>Updated Rules API in Quarkus</strong></h1><hr><hr><h3><strong>Add Error Handling to the Endpoint</strong></h3><hr><pre class="!overflow-visible"><div class="contain-inline-size rounded-md border-[0.5px] border-token-border-medium relative bg-token-sidebar-surface-primary dark:bg-gray-950"><div class="flex items-center text-token-text-secondary px-4 py-2 text-xs font-sans justify-between rounded-t-md h-9 bg-token-sidebar-surface-primary dark:bg-token-main-surface-secondary select-none">java</div><div class="sticky top-9 md:top-[5.75rem]"><div class="absolute bottom-0 right-2 flex h-9 items-center"><div class="flex items-center rounded bg-token-sidebar-surface-primary px-2 font-sans text-xs text-token-text-secondary dark:bg-token-main-surface-secondary"><span class="" data-state="closed"><button class="flex gap-1 items-center select-none py-1"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" class="icon-sm"><path fill-rule="evenodd" clip-rule="evenodd" d="M7 5C7 3.34315 8.34315 2 10 2H19C20.6569 2 22 3.34315 22 5V14C22 15.6569 20.6569 17 19 17H17V19C17 20.6569 15.6569 22 14 22H5C3.34315 22 2 20.6569 2 19V10C2 8.34315 3.34315 7 5 7H7V5ZM9 7H14C15.6569 7 17 8.34315 17 10V15H19C19.5523 15 20 14.5523 20 14V5C20 4.44772 19.5523 4 19 4H10C9.44772 4 9 4.44772 9 5V7ZM5 9C4.44772 9 4 9.44772 4 10V19C4 19.5523 4.44772 20 5 20H14C14.5523 20 15 19.5523 15 19V10C15 9.44772 14.5523 9 14 9H5Z" fill="currentColor"></path></svg>Copy code</button></span></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="!whitespace-pre hljs language-java"><span class="hljs-keyword">import</span> jakarta.inject.Inject;
<span class="hljs-keyword">import</span> jakarta.ws.rs.*;
<span class="hljs-keyword">import</span> jakarta.ws.rs.core.MediaType;
<span class="hljs-keyword">import</span> jakarta.ws.rs.core.Response;

<span class="hljs-keyword">import</span> java.util.Map;

<span class="hljs-meta">@Path("/api/rules")</span>
<span class="hljs-meta">@Produces(MediaType.APPLICATION_JSON)</span>
<span class="hljs-meta">@Consumes(MediaType.APPLICATION_JSON)</span>
<span class="hljs-keyword">public</span> <span class="hljs-keyword">class</span> <span class="hljs-title class_">RulesApi</span> {

    <span class="hljs-meta">@Inject</span>
    RuleCacheService cacheService;

    <span class="hljs-meta">@Inject</span>
    RuleExecutionService ruleService;

    <span class="hljs-meta">@POST</span>
    <span class="hljs-meta">@Path("/{fileName}/{action}")</span>
    <span class="hljs-keyword">public</span> Response <span class="hljs-title function_">executeRule</span><span class="hljs-params">(<span class="hljs-meta">@PathParam("fileName")</span> String fileName, <span class="hljs-meta">@PathParam("action")</span> String action, Map&lt;String, Object&gt; inputs)</span> {
        Map&lt;String, Object&gt; rule = cacheService.getRule(fileName, action);

        <span class="hljs-keyword">if</span> (rule == <span class="hljs-literal">null</span>) {
            <span class="hljs-keyword">return</span> Response.status(Response.Status.NOT_FOUND).entity(<span class="hljs-string">"Rule not found"</span>).build();
        }

        <span class="hljs-keyword">return</span> ruleService.executeRule(rule, inputs);
    }
}
</code></div></div></pre><hr><hr><h1><strong>How It Works</strong></h1><hr><hr><h3><strong>1. Unauthorized Access Example</strong></h3><ul><li>If a user <strong>tries to access an Admin-only action</strong>, they will receive a <strong>401 Unauthorized</strong> response.</li></ul><hr><hr><h3><strong>API Request Example</strong></h3><hr><p><strong>Request:</strong></p><pre class="!overflow-visible"><div class="contain-inline-size rounded-md border-[0.5px] border-token-border-medium relative bg-token-sidebar-surface-primary dark:bg-gray-950"><div class="flex items-center text-token-text-secondary px-4 py-2 text-xs font-sans justify-between rounded-t-md h-9 bg-token-sidebar-surface-primary dark:bg-token-main-surface-secondary select-none">bash</div><div class="sticky top-9 md:top-[5.75rem]"><div class="absolute bottom-0 right-2 flex h-9 items-center"><div class="flex items-center rounded bg-token-sidebar-surface-primary px-2 font-sans text-xs text-token-text-secondary dark:bg-token-main-surface-secondary"><span class="" data-state="closed"><button class="flex gap-1 items-center select-none py-1"><svg width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" class="icon-sm"><path fill-rule="evenodd" clip-rule="evenodd" d="M7 5C7 3.34315 8.34315 2 10 2H19C20.6569 2 22 3.34315 22 5V14C22 15.6569 20.6569 17 19 17H17V19C17 20.6569 15.6569 22 14 22H5C3.34315 22 2 20.6569 2 19V10C2 8.34315 3.34315 7 5 7H7V5ZM9 7H14C15.6569 7 17 8.34315 17 10V15H19C19.5523 15 20 14.5523 20 14V5C20 4.44772 19.5523 4 19 4H10C9.44772 4 9 4.44772 9 5V7ZM5 9C4.44772 9 4 9.44772 4 10V19C4 19.5523 4.44772 20 5 20H14C14.5523 20 15 19.5523 15 19V10C15 9.44772 14.5523 9 14 9H5Z" fill="currentColor"></path></svg>Copy code</button></span></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="!whitespace-pre hljs language-bash">POST /api/rules/user_rules/unauthorizedAccess
Content-Type: application/json
